<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="casalimpa.projeto.com.casalimpa">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ico_casalimpa"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ico_casalimpa"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".PaginaInicioActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CadastroActivity"
            android:label="@string/cadastro"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MeuPerfilActivity"
            android:label="@string/meu_perfil"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Dashboard"
            android:label="@string/dashboard"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:label="@string/login"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MeusServicosActivity"
            android:label="@string/meus_servicos"
            android:screenOrientation="portrait" />
        <activity
            android:name=".AdicionarNovoServicoActivity"
            android:label="@string/adicionar_novo_servico"
            android:screenOrientation="portrait" />
        <activity android:name=".PainelDeControleActivity" />
        <activity android:name=".AvaliacoesActivity"></activity>
    </application>

</manifest>